{
    "compilerOptions": {
        "target": "ES2023",
        "module": "ES2022",
        "moduleResolution": "node",
        "skipLibCheck": true,
        "allowJs": true,
        "strict": true,
        "strictPropertyInitialization": false,
        "rootDir": "webapp",
        "baseUrl": "./",
        "paths": {
            "fiori/walkthrough/*": [
                "webapp/*"
            ]
        }
    },
    "include": [
        "webapp/**/*"
    ]
}