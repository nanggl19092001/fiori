<mvc:View xmlns:uxap="sap.uxap"
    controllerName="fiori.walkthrough.controller.Detail"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
>
    <Page
        title="{i18n>detailPageTitle}"
        showNavButton="true"
        navButtonPress="onNavPress"
    >
    <content>
        <ObjectHeader intro="{invoice>ShipperName}" title="{invoice>ProductName}" icon="{= ${invoice>UnitPrice} > 70 ? 'sap-icon://bookmark' : 'sap-icon://bookmark-2' }"/>
        <uxap:ObjectPageLayout showAnchorBar="true">
            <uxap:ObjectPageSection ></uxap:ObjectPageSection>
        </uxap:ObjectPageLayout>
    </content>
    </Page>
</mvc:View>
